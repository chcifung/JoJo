<!--pages/index/index.wxml-->
<view class="container">
  <map id="JoMap"
    latitude="{{latitude}}" 
    longitude="{{longitude}}"
    scale="{{scale}}"
    controls="{{controls}}"
    bindcontroltap="bindcontroltap"
    polyline="{{polyline}}"
    markers="{{markers}}"
    bindmarkertap="bindmarkertap"
    bindregionchange="bindregionchange"
    show-location/>
</view>
