<!--pages/billing/index.wxml-->
<view class="container">
  <view class="classname">
    <text>当前雨伞编号:{{number}}</text>
  </view>
  <view class="time">
    <view class="time-title">
      <text>{{billing}}</text>
    </view>
    <view class="time-content">
      <text>{{days}}:{{hours}}:{{minuters}}:{{seconds}}</text>
    </view>
  </view>
  <view class="endumb">
    <button type="warn" disabled="{{disabled}}" bindtap="endUmb">结束用伞</button>
    <button type="primary" bindtap="moveToIndex">返回地图</button>
  </view>
</view>
